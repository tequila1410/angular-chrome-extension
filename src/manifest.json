{
  "name": "ZoogVPN",
  "version": "0.1",
  "description": "ZoogVPN official extension",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "management",
    "notifications",
    "privacy",
    "proxy",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "19": "assets/images/icons/disabled-19.png"
    }
  }
}
