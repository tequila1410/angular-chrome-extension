{
  "name": "Great Extension",
  "version": "1.0",
  "description": "Build an Extension with Angular",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "management",
    "notifications",
    "privacy",
    "proxy",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_popup": "index.html"
  }
}
