{
  "name": "ZoogVPN",
  "version": "0.0.1",
  "description": "ZoogVPN test extension",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "management",
    "notifications",
    "privacy",
    "proxy",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "activeTab",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "background.js",
      "runtime.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "19": "assets/images/icons/19x19-grey.png"
    }
  },
  "content_security_policy": "script-src 'self' https://dev-ng.zoogvpn.com https://www.google.com https://www.gstatic.com; object-src 'self';"
}
